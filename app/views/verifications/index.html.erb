<p id="notice"><%= notice %></p>

<h1>Verifications</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Fecha</th>
      <th>Registry</th>
      <th>Expedient</th>
      <th>Incluye expediente</th>
      <th>Coincide expediente</th>
      <th>Incluye anexos</th>
      <th>Coinciden anexos</th>
      <th>Anexos equipos de computo</th>
      <th>Anexos equipo de comunicacion</th>
      <th>Anexos material fotografico</th>
      <th>Anexos disp alm elect</th>
      <th>Anexos componentes electronicos</th>
      <th>Otros</th>
      <th>Expert</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @verifications.each do |verification| %>
      <tr>
        <td><%= verification.city %></td>
        <td><%= verification.fecha %></td>
        <td><%= verification.registry %></td>
        <td><%= verification.expedient %></td>
        <td><%= verification.incluye_expediente %></td>
        <td><%= verification.coincide_expediente %></td>
        <td><%= verification.incluye_anexos %></td>
        <td><%= verification.coinciden_anexos %></td>
        <td><%= verification.anexos_equipos_de_computo %></td>
        <td><%= verification.anexos_equipo_de_comunicacion %></td>
        <td><%= verification.anexos_material_fotografico %></td>
        <td><%= verification.anexos_disp_alm_elect %></td>
        <td><%= verification.anexos_componentes_electronicos %></td>
        <td><%= verification.otros %></td>
        <td><%= verification.expert %></td>
        <td><%= link_to 'Show', verification %></td>
        <td><%= link_to 'Edit', edit_verification_path(verification) %></td>
        <td><%= link_to 'Destroy', verification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Verification', new_verification_path %>
